{
  "name": "jest-test",
  "version": "0.0.1",
  "description": "A Jest unit test boilterplate for React & CoffeeScript",
  "repository": "https://github.com/redice/jest-react-coffeescript",
  "license": "MIT",
  "scripts": {
    "test": "jest"
  },
  "jest": {
    "scriptPreprocessor": "preprocessor.js",
    "testFileExtensions": ["coffee", "js"],
    "moduleFileExtensions": ["coffee", "js"],
    "unmockedModulePathPatterns": ["react"]
  },
  "dependencies": {
    "coffee-script": "~1.10",
    "coffee-react-transform": "~3",
    "react": "~0.13"
  }
}
